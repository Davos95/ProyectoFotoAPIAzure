@using ProyectoFotoCore.Models
@model List<PHOTO>
@{
    ViewData["Title"] = "Photos";
    Layout = "~/Views/Shared/_LayoutFront.cshtml";
}

@section styles{
    <link href="~/css/styleGridImagesFront.css" rel="stylesheet" />
}
<section id="photos" class="">
    @if (Model.Count() > 0)
    {
        foreach (PHOTO p in Model)
        {
            <img src="@p.UriAzure" />
        }
    }
</section>

